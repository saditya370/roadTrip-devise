<%dest1 = params[:destination][:starting_address]%>
<%dest2 =params[:destination][:end_address]%>
<iframe
  width="1000"
  height="450"
  style="border:2"
  loading="lazy"
  allowfullscreen
  referrerpolicy="no-referrer-when-downgrade"
  src="https://www.google.com/maps/embed/v1/directions?key=<%=Rails.application.credentials.config[:maps]%>
    &origin=<%=dest1%>
  &destination=<%=dest2%>">
</iframe>