<% @dest1 =params[:dest]%>  
<%@dest2=params[:destnext]%>
<iframe
  width="1000"
  height="450"
  style="border:2"
  loading="lazy"
  allowfullscreen
  referrerpolicy="no-referrer-when-downgrade"
  src="https://www.google.com/maps/embed/v1/directions?key=<%=Rails.application.credentials.maps%>
    &origin=<%=@dest1%>
  &destination=<%=@dest2%>">
</iframe>